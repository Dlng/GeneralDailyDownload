	</script>
	<script type="text/javascript">	
	
	<!-- move the top pagination elements outside the table top. -->
	$(".pagingtop").ready(function(){
			$(".pagingtop").insertAfter($("#tabset"));
	}); 

	<!-- move the bottom pagination elements outside the table bottom. -->
	$(".pagingbottom").ready(function(){
		$(".pagingbottom").insertAfter($("#tab-2"));				
	}); 

	<!-- switch the header rows. These are initially intentially put in the wrong order, because Datatables plugin binds -->
	<!-- the sorting events to the lower header row, and we want it on the higher one. -->
	$("#textheader").ready(function(){
			$("#emptyheader").insertAfter($("#textheader"));
	}); 

	/*
     * Purpose:  Filter fund table on the value of a select tag 
 		* Inputs:   object:obj - one of the select tags from the fund page
     *           int:column - the id of the column to filter in the table
 		*/
	function filterFundTable(obj, column) {
	//alert("object ID ::"+obj.id );
 		if (obj != null && column != null && column >= 0) {			
			if(obj.id == "fundtype") {			
				var selectedValue = obj.options[obj.selectedIndex].value; //get the selected value 
				//alert("selected value::"+obj.options[obj.selectedIndex].value ); 
				//If selected value is the heading (contains ...) than show all
				if(selectedValue.indexOf("...") != -1) {
					oTable.fnFilter("",1);				
					oTable.fnFilter("",2);
					oTable.fnFilter("",3);
				}
				else {
					oTable.fnFilter(selectedValue, column); // filter the fund table
					oTable.fnFilter("",2);
					oTable.fnFilter("",3);
				}
			}
			else if (obj.id == "fundcategory") {
				var selectedValue = obj.options[obj.selectedIndex].value; //get the selected value 
				//alert("selected value for sub fund ::"+selectedValue );
				//If selected value is the heading (contains ...) than show all
				if(selectedValue.indexOf("...") != -1) {
					oTable.fnFilter("",2);
					oTable.fnFilter("",3);
				}
				else {
					oTable.fnFilter(selectedValue, 2); // filter the fund table
					oTable.fnFilter("",3);
				}
			}
			else if (obj.id == "fundregion") {
				var selectedValue = obj.options[obj.selectedIndex].value; //get the selected value  
				//If selected value is the heading (contains ...) than show all
				if(selectedValue.indexOf("...") != -1) {					
					oTable.fnFilter("",3);
				}
				else {
					oTable.fnFilter(selectedValue, column); // filter the fund table
				}
			}			
		}
	}
	
	/*
     * Purpose:  Search fund table on fund name or ISIN
 		* Inputs:   String : searchString - user provided search string          
 		*/
	function searchFundTable(searchString) {

 		if (searchString != null) {			 				
 			// Clear the filters if the string is empty, both name and ISIN column
 			if (searchString.length == 0) {
				oTable.fnFilter(searchString, 0); 
				oTable.fnFilter(searchString, 4); 
			}

			// If the searchstring is 4 or longer, determine if name or ISIN and filter on the 
			// corresponding column. 
 			else if (searchString.length >= 4) {
				var column = 0;						
				var ISINpattern = /[a-zA-Z]{2}[0-9]{2}/; 
				if(ISINpattern.test(searchString)) {
					column = 4;						
				}
				oTable.fnFilter(searchString, column); // filter the fund table
			}
		}
	}			

	function fnShowHide( iCol )
	{
		/* Get the DataTables object again - this is not a recreation, just a get of the object */
		var oTable = $('#page').dataTable();
		
		var bVis = oTable.fnSettings().aoColumns[iCol].bVisible;
		oTable.fnSetColumnVis( iCol, bVis ? false : true );
	}			
</script>	
				
			<!-- SS_END_SNIPPET(fragment15,Fund_Table)-->
  	
