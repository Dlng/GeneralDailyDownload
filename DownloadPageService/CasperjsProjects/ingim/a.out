			, "bJQueryUI": true
			, "sDom": '<"pagingtop"p>rt<"listing"i><"clear">'		
			, "oLanguage": {
					"sZeroRecords": "No matching funds found"
					, "sInfo": "Showing _START_ to _END_ of _TOTAL_ funds"
					, "sInfoEmpty": "Showing 0 to 0 of 0 funds"
					, "sInfoFiltered": "(filtered from _MAX_ total funds)" 
					, "oPaginate": {
							"sFirst":    "First"
							, "sPrevious": "Previous"
							, "sNext":     "Next"
							, "sLast":     "Last"
						}
				}
			, "fnDrawCallback": function() {
                <!-- code to hide pagination if 0 or 1 pages to display -->                  	    
                if (Math.ceil((this.fnSettings().fnRecordsDisplay()) / this.fnSettings()._iDisplayLength) > 1)  {
                        $(".dataTables_paginate").css("display", "block");	
						$(".dataTables_length").css("display", "block");
						$(".dataTables_filter").css("display", "block");						
                	} else {
                        $(".dataTables_paginate").css("display", "none");
